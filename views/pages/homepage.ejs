    <v-flex class="d-flex" <%=me ? 'xs4' : 'xs6'%>>
    <% if (me) { %>
      <v-form class="text-xs-center" method="POST" action="/search">
        <input type="hidden" name="_csrf" :value="csrf" />
        <input type="hidden" name="gender" :value="JSON.stringify(me.data.gender)" />
        <input type="hidden" name="lookingFor" :value="JSON.stringify(me.data.lookingFor)" />
        <input type="hidden" name="place" :value="JSON.stringify(me.data.place)" />
        <input type="hidden" name="purpose" :value="JSON.stringify(me.data.purpose)" />
        <input type="hidden" name="ageRange" :value="JSON.stringify(me.data.ageRange)" />
        <v-btn 
          @click.stop="findMore = true"
          :disabled="findMore"
          :loading="findMore"
          color="info" 
          type="submit"
          round
          large>Найти больше анкет</v-btn>
      </v-form>
    <% }else { %>
      <v-btn href="/signup" large dark round block color="green lighten-1">Зарегистрироваться и начать знакомиться</v-btn>
    <% } %>
